.timeline-bg{
    background: repeat-y 365rpx url('data:image/jpg;base64,');
    margin-top: 50rpx;
    margin-bottom: 50rpx;
}
.timeline-year{
    width: 120rpx;
    height: 120rpx;
    border-radius: 100rpx;
    background-color: #0099cc;
    text-align: center;
    border: solid 5px #fff;
    margin-bottom: 100rpx;
}
.timeline-year_text{
    padding-top: 34rpx;
    color: #fff;
    font-weight: bold;
    font-size: medium;
}
.timeline-center{
    margin-left: 300rpx;
}
.text_white{
    color:#fff;
}
.timeline-box{
    margin-top: -20rpx;
}
.timeline-right{
    margin-left: 342rpx;
}
.timeline-left{
    margin-right: 352rpx;
    text-align: right;
}
.timeline-event-point{
    margin-top: 10rpx;
    width: 36rpx;
    height: 36rpx;
    border-radius: 100rpx;
    background-color: #999;
    border: solid 5px #fff;
}
.timeline-event{
    margin-left: 20rpx;
    padding-bottom: 10rpx;
    background-color: #fff;
}
.event-box{
    border: 1px solid #808080;
    margin: 0 20rpx;
    border-radius: 10rpx;

}
.event-hd{
    background-color: #999;
    border-bottom: solid 1px;
    padding: 6rpx 16rpx;
}
.event-bd{
    padding: 6rpx 16rpx;
    font-size: 14px;
}
.timeline-box-main{
    margin: 20rpx 50rpx 60rpx;
}
.event-hd-right{
    text-align: right;
}
.small{    
    font-size:small;
}
.gray{
    color: gray;
}
.scale0{
    transform:scaleX(0);
}
.scale1{
    transform:scaleX(1);
}
.boxshow{
    /*animation: boxshow 2s;*/
    transform:scaleY(0);
}
@keyframes boxshow
{
from {opacity: 0; }
to {opacity: 1;}
}
.color_0{
    background-color: #81C2D6;
    color: #fff;
}
.color_1{
    background-color: #8192D6;
    color: #fff;
}
.color_2{
    background-color: #D9B3E6;
    color: #fff;
}
.color_3{
    background-color: #83FCD8;
    color: #808080;
}
.color_4{
    background-color: #DCF7A1;
    color: #808080;
}